#include <mega128a.h>
unsigned char led =0xFE;

void main(void)
{
    DDRC=0xFF;
    PORTC= led;
    
    TCCR0=0x0f;
    TIMSK=0x02;
    OCR0=155;
    SREG=0xff;
    
    while(1);
}

interrupt [TIM0_COMP] void timer_comp0 (void)
{

}